{
  "name": "dds-web-test",
  "version": "0.1.5",
  "description": "Dodam Design System - React component library with styled-components",
  "keywords": [
    "react",
    "design-system",
    "ui",
    "components",
    "styled-components",
    "typescript",
    "nextjs"
  ],
  "author": "Team B1ND",
  "license": "MIT",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "private": false,
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./icons": {
      "types": "./dist/types/icons.d.ts",
      "import": "./dist/icons.js",
      "require": "./dist/icons.cjs"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@changesets/cli": "^2.27.1",
    "@microsoft/api-extractor": "^7.55.2",
    "@storybook/addon-interactions": "^8.4.1",
    "@storybook/addon-links": "^8.4.1",
    "@storybook/addon-onboarding": "^8.4.1",
    "@storybook/addon-themes": "^8.4.1",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/react-webpack5": "^8.4.1",
    "@tanstack/react-query": "^5.90.19",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.34",
    "@types/typescript": "^2.0.0",
    "esbuild": "^0.27.2",
    "prettier": "^3.2.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^8.4.1",
    "styled-components": "^6.1.8",
    "swc-loader": "^0.2.6",
    "tsup": "^8.5.1",
    "typescript": "^5.4.2"
  },
  "scripts": {
    "build": "rm -rf dist && tsup && tsc --declaration --emitDeclarationOnly --outDir dist/types && node scripts/add-use-client.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team-B1ND/dds-web.git"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "styled-components": "^6.0.0"
  }
}
